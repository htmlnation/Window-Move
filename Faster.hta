<HTA:APPLICATION
  APPLICATIONNAME="ation"
  ID="MyHTMLapplication"
  VERSION="1.0"
  CAPTION="no"
  SYSMENU="no"
  scroll="no"
  showInTaskbar="yes"
  MAXIMIZEBUTTON="no"
  MINIMIZEBUTTON="no"/>
 
 
</head>

<script language="VBScript">
window.resizeTo 240,280
 
 
Sub Window_OnLoad
 Dim a
 Dim b
 a = 0
 a1(a)
 idTimer = window.setTimeout("a1", 500, "VBScript")
End Sub

Sub a1(a)
 Do
  Do
   window.moveTo a, b
   a = a + 1
   idTimer = window.setTimeout("a1",11500, "VBScript")
  Loop Until a = 100
   Do
    window.moveTo a, b
    b = b + 1
   Loop Until b = 10
 Loop Until a = 100 & b = 100
 me.close
End Sub



 
</script>

